<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="org.lttng.studio.bundle"
            application="org.eclipse.e4.ui.workbench.swt.E4Application">
         <property
               name="appName"
               value="org.lttng.studio.bundle">
         </property>
         <property
               name="applicationXMI"
               value="org.lttng.studio.bundle/Application.e4xmi">
         </property>
         <property
               name="applicationCSS"
               value="platform:/plugin/org.lttng.studio.bundle/css/default.css">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.eclipse.linuxtools.lttng2.ui.views.category"
            class="org.lttng.studio.ui.widget.DemoSequenceDiagramView"
            id="org.lttng.studio.bundle.view1"
            name="SDView Demo"
            restorable="true">
      </view>
   </extension>

</plugin>
